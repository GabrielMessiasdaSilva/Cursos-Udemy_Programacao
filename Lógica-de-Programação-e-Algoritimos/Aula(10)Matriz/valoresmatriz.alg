algoritmo "valoresmatriz"
// Função :
// Autor :
// Data : 09/03/2023
// Seção de Declarações
var
m:vetor[1..4,1..4]de inteiro
l,c : inteiro
inicio
para l <- 1 ate 4 faca
   para c <- 1 ate 4 faca
      escreva("Digite o valor da posição [",1,",",c,"]:")
      leia (m[l,c])
   fimpara
fimpara
fimalgoritmo
